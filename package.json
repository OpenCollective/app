{
  "name": "react-starter-es6-babel",
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production browserify main.js | uglifyjs -cm > public/bundle.js",
    "start": "ecstatic -p 8000 public",
    "watch": "NODE_ENV=development watchify main.js -o public/bundle.js -dv",
    "hint": "./node_modules/.bin/jscs **/*.js --config=.jscsrc",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "babelify": "^6.1.3",
    "basscss": "^7.0.4",
    "browserify": "^10.2.6",
    "ecstatic": "~0.8.0",
    "envify": "^3.4.0",
    "express": "^4.13.3",
    "isomorphic-fetch": "^2.1.1",
    "jwt-decode": "^1.4.0",
    "lodash": "^3.10.1",
    "normalizr": "^1.0.0",
    "react": "~0.13.3",
    "react-intl": "^1.2.0",
    "react-redux": "^3.0.1",
    "react-router": "^1.0.0-rc1",
    "redux": "^3.0.2",
    "redux-logger": "^2.0.0",
    "redux-router": "^1.0.0-beta3",
    "redux-thunk": "^1.0.0",
    "uglify-js": "^2.4.24",
    "watchify": "^3.2.3"
  },
  "devDependencies": {
    "jscs": "^2.1.1",
    "redux-devtools": "^2.1.5"
  }
}
