{
  "name": "opencollective-app",
  "version": "0.1.20",
  "engines": {
    "node": "6.2.2",
    "npm": "3.9.5"
  },
  "browserify": {
    "transform": [
      "babelify",
      "envify"
    ]
  },
  "scripts": {
    "start": "NODE_CONFIG_DIR=./server/config node server",
    "dev": "gulp build:dev && PORT=3030 nf start -j Procfile_dev",
    "hint": "eslint '**/*.js' --quiet",
    "pretest": "npm run hint",
    "test": "npm run test:unit",
    "test:unit": "NODE_CONFIG_DIR=./server/config mocha test/unit/ --opts test/unit/mocha.opts --reporter-options mochaFile=$(bash scripts/test_output_dir.sh junit)/junit.xml",
    "posttest": "npm run test:e2e",
    "test:e2e": "npm run test:e2e:exec api:run app:run app:testE2E",
    "test:e2e:exec": "npm run rexec:api test_e2e.sh",
    "nightwatch": "NODE_CONFIG_DIR=./server/config ./node_modules/nightwatch/bin/nightwatch --config nightwatch.json",
    "build": "gulp build && npm run build:js",
    "build:js": "mkdir -p frontend/dist/js && browserify frontend/src/client.js | uglifyjs -cm > frontend/dist/js/bundle.js",
    "watch:js": "mkdir -p frontend/dist/js && watchify frontend/src/client.js -o frontend/dist/js/bundle.js -dv",
    "watch:test": "mocha test/unit/ --opts test/unit/mocha.opts --watch",
    "watch:server": "NODE_CONFIG_DIR=./server/config nodemon -e js,ejs,hbs -w server server/index.js -w frontend/src",
    "postinstall": "bash scripts/postinstall.sh",
    "git:clean": "git branch --merged | grep -v \\* | grep -v master | grep -v dev | xargs -n 1 git branch -d",
    "deploy:staging": "bash -x scripts/deploy.sh staging",
    "deploy:production": "bash -x scripts/deploy.sh production",
    "rexec:api": "bash scripts/remote_exec_api.sh",
    "install:debs": "npm run rexec:api install_apt_get_debs.sh"
  },
  "dependencies": {
    "autoprefixer": "8.5.0",
    "babel-core": "6.26.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babelify": "8.0.0",
    "basscss": "8.0.4",
    "body-parser": "1.18.3",
    "browserify": "16.2.2",
    "cache-bust": "0.0.2",
    "classnames": "2.2.5",
    "config": "1.30.0",
    "cssnano": "3.10.0",
    "cssnext": "1.8.4",
    "ecstatic": "3.2.0",
    "ejs": "2.6.1",
    "envify": "4.1.0",
    "express": "4.16.3",
    "express-server-status": "1.0.3",
    "gulp": "3.9.1",
    "gulp-changed": "3.2.0",
    "gulp-load-plugins": "1.5.0",
    "gulp-postcss": "7.0.1",
    "gulp-responsive": "2.10.0",
    "history": "4.7.2",
    "isomorphic-fetch": "2.2.1",
    "joi": "13.3.0",
    "jwt-decode": "2.2.0",
    "jwt-simple": "0.5.1",
    "lodash": "4.17.10",
    "marked": "0.3.19",
    "moment": "2.22.1",
    "morgan": "1.9.0",
    "normalizr": "3.2.4",
    "numbro": "2.0.6",
    "postcss-cssnext": "3.1.0",
    "postcss-discard-comments": "2.0.4",
    "postcss-import": "11.1.0",
    "query-string": "6.1.0",
    "react": "16.3.2",
    "react-body-classname": "1.2.0",
    "react-datepicker": "1.4.1",
    "react-dom": "16.3.2",
    "react-onclickoutside": "6.7.1",
    "react-radio-group": "3.0.3",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-stripe-checkout": "2.6.3",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-router": "2.1.2",
    "redux-thunk": "2.2.0",
    "request": "2.86.0",
    "robots.txt": "1.1.0",
    "serve-favicon": "2.5.0",
    "uglify-js": "3.3.25"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-eslint": "8.2.3",
    "chai": "4.1.2",
    "chai-spies": "1.0.0",
    "chromedriver": "2.38.3",
    "eslint": "4.19.1",
    "eslint-config-opencollective": "1.2.1",
    "eslint-config-rackt": "1.1.1",
    "eslint-plugin-react": "7.8.2",
    "expect": "22.4.3",
    "jscs": "3.0.7",
    "jsdom": "11.10.0",
    "localmockage": "0.0.1",
    "loose-envify": "1.3.1",
    "mocha": "5.1.1",
    "mocha-circleci-reporter": "0.0.3",
    "nightwatch": "0.9.21",
    "nock": "9.2.6",
    "react-addons-test-utils": "15.6.2",
    "react-unit": "3.0.3",
    "redux-devtools": "3.4.1",
    "redux-mock-store": "1.5.1",
    "sinon": "5.0.7",
    "supertest": "3.1.0",
    "watchify": "3.11.0"
  },
  "License": "MIT"
}
